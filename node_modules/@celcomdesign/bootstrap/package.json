{
  "name": "@celcomdesign/bootstrap",
  "description": "Prisma 2. Build with Bootstrap.",
  "homepage": "https://celcomdesign.gitlab.io/system/bootstrap/",
  "author": "Celcom Design",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/celcomdesign/system/bootstrap.git"
  },
  "bugs": {
    "url": "https://gitlab.com/celcomdesign/system/bootstrap/issues"
  },
  "scripts": {
    "build": "parcel build ./src/docs/*.html --dist-dir public --public-url https://celcomdesign.gitlab.io/system/bootstrap --no-optimize",
    "clean": "rm -rf .parcel-cache public/",
    "compile": "npm-run-all --parallel css js",
    "css-compile": "node-sass --include-path node_modules --output-style expanded --source-map true --source-map-contents true --precision 6 src/scss -o dist/css/",
    "css-minify": "cleancss -O1 --format breakWith=lf --with-rebase --source-map --source-map-inline-sources --output dist/css/ --batch --batch-suffix \".min\" \"dist/css/*.css\" \"!dist/css/*.min.css\"",
    "css-lint": "stylelint src/scss/",
    "css-prefix": "postcss --replace dist/css/prisma.css --use autoprefixer --map",
    "css-purge": "purgecss --keyframes --css dist/css/prisma.css --content index.html \"node_modules/bootstrap/js/dist/{util,modal}.js\" --output dist/css/",
    "css": "npm-run-all css-compile css-prefix css-minify",
    "js": "webpack",
    "server": "parcel serve ./src/docs/*.html --dist-dir public --port 3000",
    "start": "npm-run-all --parallel watch server",
    "watch": "nodemon -e html,scss,js -x \"npm run compile\"",
    "test": "npm run css-lint && npm run css"
  },
  "dependencies": {
    "@celcomdesign/iconography": "^2.0.3",
    "@swc/helpers": "^0.2.13",
    "bootstrap": "~4.6.0",
    "clipboard": "^2.0.8",
    "jquery": "~3.6.0",
    "popper.js": "~1.16.1",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.6",
    "clean-css-cli": "^5.3.0",
    "node-sass": "^6.0.1",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0-rc.0",
    "postcss": "^8.3.1",
    "postcss-cli": "^8.3.1",
    "purgecss": "^4.0.3",
    "serve": "^12.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-twbs-bootstrap": "^2.2.1",
    "terser-webpack-plugin": "^5.1.4",
    "webpack": "^5.41.1",
    "webpack-cli": "^4.7.2"
  },
  "files": [
    "dist/{css,js}/*.{css,js,map}",
    "src/scss/**/*.scss",
    "src/js/*.js"
  ],
  "sass": "src/scss/prisma.scss",
  "style": "dist/css/prisma.css",
  "main": "dist/js/prisma.js",
  "targets": {
    "main": false
  },
  "version": "4.6.0-beta"
}
